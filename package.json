{
    "name": "vue-i18n-translator",
    "version": "1.0.2",
    "description": "Translate Vue i18n JSON files using OpenAI with support for Nuxt projects",
    "type": "module",
    "private": false,
    "main": "dist/index.js",
    "bin": {
        "vue-i18n-translator": "./dist/index.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "prepare": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "execute": "node dist/index.js"
    },
    "keywords": [
        "i18n",
        "translation",
        "vue",
        "nuxt",
        "openai",
        "cli",
        "github-action"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "chalk": "^5.3.0",
        "commander": "^11.0.0",
        "dotenv": "^16.3.1",
        "fs-extra": "^11.1.1",
        "inquirer": "^8.2.5",
        "openai": "^4.0.0",
        "ora": "^5.4.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.1",
        "@types/inquirer": "^8.2.6",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.5.7",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "eslint": "^8.45.0",
        "jest": "^29.6.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/deepl-translator-vue-i18n.git"
    }
